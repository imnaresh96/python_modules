   "source": [
    "def number(*num):\n",
    "    odd=[]\n",
    "    even=[]\n",
    "    for i in num:\n",
    "        if i%2==0:\n",
    "            even.append(i)\n",
    "        else:\n",
    "            odd.append(i)\n",
    "    else:\n",
    "        print(f'odd= {odd} ')\n",
    "        print(f'even={even}')"
   ]
